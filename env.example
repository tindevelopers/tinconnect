# Supabase Configuration - Production
VITE_SUPABASE_URL=your_production_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Server-side Supabase - Production (for API routes)
SUPABASE_URL=your_production_supabase_project_url
SUPABASE_ANON_KEY=your_production_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_supabase_service_role_key

# Supabase Configuration - Preview/Staging
VITE_SUPABASE_URL_PREVIEW=your_staging_supabase_project_url
VITE_SUPABASE_ANON_KEY_PREVIEW=your_staging_supabase_anon_key

# Server-side Supabase - Preview/Staging (for API routes)
SUPABASE_URL_PREVIEW=your_staging_supabase_project_url
SUPABASE_ANON_KEY_PREVIEW=your_staging_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY_PREVIEW=your_staging_supabase_service_role_key

# AWS Configuration - Production (for Chime SDK)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_production_aws_access_key
AWS_SECRET_ACCESS_KEY=your_production_aws_secret_key

# AWS Configuration - Preview/Staging (for Chime SDK)
AWS_REGION_PREVIEW=us-east-1
AWS_ACCESS_KEY_ID_PREVIEW=your_staging_aws_access_key
AWS_SECRET_ACCESS_KEY_PREVIEW=your_staging_aws_secret_key

# Optional: Custom ping message
PING_MESSAGE=pong
